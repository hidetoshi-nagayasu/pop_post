<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_with(model: @user) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div>
  <%= f.label :email %>
  <%= f.email_field :email, class: "form-control" %>
  </div>

  <div>
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control" %>
  </div>

  <div>
  <%= f.submit %>
  </div>
<% end %>
